    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with \"local\" config",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/app",
            "program": "powerproxy.py",
            "env": {
                "POWERPROXY_CONFIG_FILE": "config.local.yaml",
                "POWERPROXY_CONFIG_PATH": "${workspaceFolder}/config"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
